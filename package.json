{
  "name": "hucomdic",
  "version": "0.1.16",
  "description": "The human-computer dictionary is a service that translates natural language requests into programming language. This package contains a command like application for uploading dictionary files to the hucomdic.org server, a command line bot for using the dictionary and some additional library for parsing intermediate format (nnt).",
  "main": "index.js",
  "bin": {
    "hucomdic": "cli.js",
    "bot": "bot.js"
  },
  "dependencies": {
    "cli-table2": "^0.2.0",
    "colors": "^1.1.2",
    "dodido-client": "^0.1.4",
    "fs-extra": "^2.0.0",
    "json-yaml": "^0.1.0",
    "klaw": "^1.3.1",
    "minimist": "^1.2.0",
    "pegjs": "^0.10.0",
    "pluralize": "^3.1.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "tap": "",
    "pegjs": "0.10.0"
  },
  "scripts": {
    "test": "tap test/*.js",
    "build": "pegjs -o parse.js src/pattern.pegjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodidoio/hucomdic.git"
  },
  "keywords": [
    "human-computer-dictionary",
    "hucomdic",
    "bot",
    "chatbot"
  ],
  "author": "Elchanan Shor",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/dodidoio/hucomdic/issues"
  },
  "homepage": "https://github.com/dodidoio/hucomdic#readme"
}
